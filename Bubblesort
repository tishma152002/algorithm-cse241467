#include<bits/stdc++.h>
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n,q;
    cin>>n>>q;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }

    int pfx[n+1];
    pfx[0]={0};
    for(int i=1;i<n;i++){
        pfx[i]=pfx[i-1]+arr[i];
    }

    while(q--){
        int l,r;
        cin>>l>>r;
        int sum=0;

        if(l==0) sum=pfx[r];
        else{
            sum=pfx[r]-pfx[l-1];
        }

        cout<<sum<<endl;
    }

    return 0;
}
